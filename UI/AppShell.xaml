<?xml version="1.0" encoding="UTF-8" ?>
<Shell
    x:Class="UI.AppShell"
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:views="clr-namespace:UI.Views"
    xmlns:mainviews="clr-namespace:UI.Views.MainPageViews"
    Title="TreeGlyph">

    <ShellContent
        ContentTemplate="{DataTemplate mainviews:MainPage}"
        Route="MainPage" />

        <ShellContent
        ContentTemplate="{DataTemplate views:AboutPage}"
        Route="AboutPage" />

        <ShellContent
        ContentTemplate="{DataTemplate views:LogViewPage}"
        Route="LogViewPage" />


</Shell>